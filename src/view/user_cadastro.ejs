    <div class="content">
        <form action="/users/cadastrar" method="post">
            <h1>Cadastro</h1>
            <table>
                <tr>
                    <td><label for="nome">Nome</label>
                    <input type="text" name="nome"></td>
                </tr>
                <tr>
